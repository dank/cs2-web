  "name": "Problem 2",
  "analysis": {
    "objectives": [
      "Create an application that calculates the BMI of a person given their weight and height.",
      "Give the user the BMI standards"
    ],
    "inputs": [
      "Weight - A positive integer, in pounds",
      "Height - A positive integer, in inches squared"
    ],
    "outputs": [
      "Users provided height and weight",
      "Users calculated BMI to 2 decimal places",
      "BMI standard, provided"
    ],
    "assumptions": [
      "Weight is positive and an integer",
      "Height is positive and an integer"
    ],
    "constraints": [
      "Output BMI to 2 decimals",
      "No negative inputs",
      "Inputs are integers"
    ],
    "design": [
      "Ask user to input weight",
      "Store weight as Absolute value int",
      "Ask user to input height",
      "Store height as Absolute value int",
      "Calculate BMI score using given method",
      "Print BMI Score with 2 decimals",
      "Print BMI standards"
    ]
  },
